@model IEnumerable<DebtCalculatorWebApp.Models.AutoLoan>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AutoLoanAmount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AutoLoanTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AutoLoanApr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecordDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AutoLoanAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AutoLoanTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AutoLoanApr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecordDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AutoLoanId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AutoLoanId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AutoLoanId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
